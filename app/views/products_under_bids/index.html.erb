<p id="notice"><%= notice %></p>

<h1>Products Under Bids</h1>

<table>
  <thead>
    <tr>
      <th>Product bid</th>
      <th>Product</th>
      <th>Minimum bidding price</th>
      <th>Bid status</th>
      <th>Sell status</th>
      <th>Bid start date</th>
      <th>Bid start time</th>
      <th>Bid end date</th>
      <th>Bid end time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @products_under_bids.each do |products_under_bid| %>
      <tr>
        <td><%= products_under_bid.product_bid_id %></td>
        <td><%= products_under_bid.product_id %></td>
        <td><%= products_under_bid.minimum_bidding_price %></td>
        <td><%= products_under_bid.bid_status %></td>
        <td><%= products_under_bid.sell_status %></td>
        <td><%= products_under_bid.bid_start_date %></td>
        <td><%= products_under_bid.bid_start_time %></td>
        <td><%= products_under_bid.bid_end_date %></td>
        <td><%= products_under_bid.bid_end_time %></td>
        <td><%= link_to 'Show', products_under_bid %></td>
        <td><%= link_to 'Edit', edit_products_under_bid_path(products_under_bid) %></td>
        <td><%= link_to 'Destroy', products_under_bid, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Products Under Bid', new_products_under_bid_path %>
